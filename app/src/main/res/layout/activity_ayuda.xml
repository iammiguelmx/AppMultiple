<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AyudaActivity" android:background="#CBD84C">

    <TextView
            android:text="Ayuda "
            android:textSize="12pt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="50dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="50dp"/>
    <Button
            android:text="Regresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnRegresaAyuda"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="50dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="50dp" android:background="#FFFFFF"/>
</androidx.constraintlayout.widget.ConstraintLayout>